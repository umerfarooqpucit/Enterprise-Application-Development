@{
    ViewBag.Title = "UserHome";
    Layout = "~/Views/Shared/_Shared.cshtml";
}

<div class="col-lg-12">
    <center>
        <h1>
            Welcome @Session["login"]
        </h1>
    </center>
</div>
<div class="col-lg-11 col-lg-offset-1" >
    <div style="border:1px solid black;height:201px;width:201px;">
        <img src="@Url.Content("/Images/" + ViewBag.ImgName)" alt="profile image" style="height:200px; width:200px" id="imgDisp" /><br />
    </div>
</div>
<div class="col-lg-12">
    <br />
</div>
<div class="col-lg-11 col-lg-offset-1"  >
    <a href="@Url.Content("/User/Logout")" class="btn btn-md btn-default">Logout</a>
    <a href="@Url.Content("/User/EditUser")" class="btn btn-md btn-default" >Edit Profile</a>
</div>
